            <field name="run_as_queue_job" eval="True"/>
            <field name="no_parallel_queue_job_run" eval="True"/>
            <field name="channel_id" ref="drop.channel_workout"/>
            <field name="priority">10</field>
        </record>
    </data>
</odoo>