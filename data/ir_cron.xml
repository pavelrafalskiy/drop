<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_cron_check_workouts" model="ir.cron">
            <field name="name">Workout: Notification Scanner</field>
            <field name="model_id" ref="model_workout_event"/>
            <field name="state">code</field>
            <field name="code">model.cron_check_upcoming_workouts()</field>
            <field name="interval_number">5</field>
